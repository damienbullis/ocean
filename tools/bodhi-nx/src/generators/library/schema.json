{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "library",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for the library?",
      "pattern": "^[a-zA-Z].*$"
    }
  },
  "required": ["name"]
}
